<launch>

  <!-- (1) launch dviz_headless -->
  <include file="$(find dviz_core)/launch/dviz_headless.launch" />

  <!-- (2) run the tf2_web_republisher node -->
  <node name="tf2_web_republisher" pkg="tf2_web_republisher" type="tf2_web_republisher" output="log" />

  <!-- (3) launch rosbridge_server -->
  <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" />

</launch>